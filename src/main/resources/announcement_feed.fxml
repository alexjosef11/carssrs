<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="465.0" prefWidth="650.0" stylesheets="@buttonstyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="user.registration.controllers.AnnouncementsFeedController">
    <children>
      <ListView fx:id="Card" layoutX="131.0" layoutY="96.0" onMouseClicked="#handlemouseclick" prefHeight="253.0" prefWidth="297.0" style="-fx-background-color: transparent;" />
      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="205.0" layoutX="434.0" layoutY="96.0" pickOnBounds="true" preserveRatio="true" />
        <Pane prefHeight="465.0" prefWidth="131.0" style="-fx-background-color: #5985ff;">
            <children>
                <ImageView fitHeight="176.0" fitWidth="246.0" layoutX="-29.0" layoutY="-23.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Logo.png" />
                    </image>
                </ImageView>
                <Button layoutY="424.0" mnemonicParsing="false" onAction="#backtosomething" style="-fx-background-color: transparent;" styleClass="back-next-button" stylesheets="@buttonstyle.css" text="Back" textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font name="Calibri Bold Italic" size="16.0" />
                    </font>
                </Button>
            </children>
        </Pane>
        <TextField alignment="BOTTOM_CENTER" editable="false" layoutX="143.0" layoutY="14.0" mouseTransparent="true" prefHeight="35.0" prefWidth="182.0" style="-fx-background-color: transparent; -fx-border-color: #5985ff; -fx-border-width: 0px 0px 2px 0px;" text="Announcements Feed">
            <font>
                <Font name="Calibri Italic" size="18.0" />
            </font>
            <effect>
                <Blend />
            </effect>
        </TextField>
        <HBox alignment="TOP_RIGHT" layoutX="450.0" prefHeight="35.0" prefWidth="200.0">
            <children>
                <Button fx:id="minimizeField" contentDisplay="TOP" mnemonicParsing="false" onAction="#minimizeWindow" prefHeight="24.0" prefWidth="35.0" style="-fx-background-color: transparent;" text="...." textFill="#5985ff">
                    <font>
                        <Font name="System Bold" size="11.0" />
                    </font>
                </Button>
                <Button fx:id="closeField" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#closeWindow" prefHeight="27.0" prefWidth="25.0" style="-fx-background-color: transparent;" text="X" textAlignment="CENTER" textFill="#5985ff" textOverrun="CLIP">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
        </HBox>
        <TextField fx:id="roleField" editable="false" layoutX="325.0" layoutY="19.0" mouseTransparent="true" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 0px 0px;">
            <font>
                <Font name="Calibri Bold Italic" size="13.0" />
            </font>
        </TextField>
        <Button layoutX="517.0" layoutY="389.0" onAction="#handleMakeOffer" prefHeight="36.0" prefWidth="109.0" style="-fx-background-color: #5895ff;" styleClass="register-button" stylesheets="@buttonstyle.css" text="Make Offer" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Calibri Bold Italic" size="15.0" />
            </font>
        </Button>
      <Button layoutX="155.0" layoutY="389.0" onAction="#gotoAddAnnouncement" prefHeight="36.0" prefWidth="176.0" style="-fx-background-color: #5895ff;" styleClass="register-button" stylesheets="@buttonstyle.css" text="Add new Announcement" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Calibri Bold Italic" size="15.0" />
         </font>
      </Button>
      <Button layoutX="374.0" layoutY="389.0" onAction="#gotoOfferTab" prefHeight="36.0" prefWidth="109.0" style="-fx-background-color: #5895ff;" styleClass="register-button" stylesheets="@buttonstyle.css" text="Offer Tab" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Calibri Bold Italic" size="15.0" />
         </font>
      </Button>
      <Text fx:id="availability" fill="#5985ff" layoutX="458.0" layoutY="303.0" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="158.0">
         <font>
            <Font name="Calibri" size="13.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
