<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="465.0" prefWidth="650.0" stylesheets="@buttonstyle.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="user.registration.controllers.FeedbackController">
    <children>
        <Pane prefHeight="465.0" prefWidth="131.0" style="-fx-background-color: #5985ff;">
            <children>
                <ImageView fitHeight="176.0" fitWidth="246.0" layoutX="-29.0" layoutY="-23.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@Logo.png" />
                    </image>
                </ImageView>
                <Button layoutY="424.0" mnemonicParsing="false" onAction="#goBackToRoleChooseScene" style="-fx-background-color: transparent;" styleClass="back-next-button" stylesheets="@buttonstyle.css" text="Back" textAlignment="CENTER" textFill="WHITE">
                    <font>
                        <Font name="Calibri Bold Italic" size="16.0" />
                    </font>
                </Button>
            </children>
        </Pane>
        <TextField alignment="BOTTOM_CENTER" editable="false" layoutX="143.0" layoutY="14.0" mouseTransparent="true" prefHeight="35.0" prefWidth="182.0" promptText="Enter specific PIN for Application Owner" style="-fx-background-color: transparent; -fx-border-color: #5985ff; -fx-border-width: 0px 0px 2px 0px;" text="Feedback">
            <font>
                <Font name="Calibri Italic" size="18.0" />
            </font>
            <effect>
                <Blend />
            </effect>
        </TextField>
        <HBox alignment="TOP_RIGHT" layoutX="450.0" prefHeight="35.0" prefWidth="200.0">
            <children>
                <Button fx:id="minimizeField" contentDisplay="TOP" mnemonicParsing="false" onAction="#minimizeWindow" prefHeight="24.0" prefWidth="35.0" style="-fx-background-color: transparent;" text="...." textFill="#5985ff">
                    <font>
                        <Font name="System Bold" size="11.0" />
                    </font>
                </Button>
                <Button fx:id="closeField" contentDisplay="BOTTOM" mnemonicParsing="false" onAction="#closeWindow" prefHeight="27.0" prefWidth="25.0" style="-fx-background-color: transparent;" text="X" textAlignment="CENTER" textFill="#5985ff" textOverrun="CLIP">
                    <font>
                        <Font size="14.0" />
                    </font>
                </Button>
            </children>
        </HBox>
        <TextField editable="false" layoutX="131.0" layoutY="63.0" mouseTransparent="true" prefHeight="27.0" prefWidth="461.0" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 0px 0px;" text="Please complete this feedback to help us improving our app">
            <font>
                <Font name="Calibri Bold Italic" size="14.0" />
            </font>
        </TextField>
        <Button layoutX="299.0" layoutY="388.0" onAction="#handleFeedbackAction" prefHeight="36.0" prefWidth="76.0" style="-fx-background-color: #5895ff;" styleClass="register-button" stylesheets="@buttonstyle.css" text="Done" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Calibri Bold Italic" size="15.0" />
            </font>
        </Button>
      <TextField fx:id="question1" editable="false" layoutX="137.0" layoutY="100.0" mouseTransparent="true" prefHeight="27.0" prefWidth="366.0" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 0px 0px;" text="1) Was the app usefull for you?">
         <font>
            <Font name="Calibri Bold Italic" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="response1" layoutX="143.0" layoutY="127.0" prefHeight="25.0" prefWidth="297.0" promptText="response" />
      <TextField fx:id="response2" layoutX="143.0" layoutY="194.0" prefHeight="25.0" prefWidth="297.0" promptText="response" />
      <TextField fx:id="question2" editable="false" layoutX="137.0" layoutY="167.0" mouseTransparent="true" prefHeight="27.0" prefWidth="366.0" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 0px 0px;" text="2) Do you like the app design?">
         <font>
            <Font name="Calibri Bold Italic" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="question3" editable="false" layoutX="137.0" layoutY="233.0" mouseTransparent="true" prefHeight="27.0" prefWidth="366.0" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 0px 0px;" text="3) What would you change on the app?">
         <font>
            <Font name="Calibri Bold Italic" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="response3" layoutX="143.0" layoutY="260.0" prefHeight="25.0" prefWidth="297.0" promptText="response" />
      <TextField fx:id="question4" editable="false" layoutX="137.0" layoutY="304.0" mouseTransparent="true" prefHeight="27.0" prefWidth="366.0" style="-fx-background-color: transparent; -fx-border-width: 0px 0px 0px 0px;" text="4)  How would you rate the app? (from 1 to 5)">
         <font>
            <Font name="Calibri Bold Italic" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="response4" layoutX="143.0" layoutY="331.0" prefHeight="25.0" prefWidth="297.0" promptText="response" />
    </children>
</AnchorPane>
